<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>

<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml" fx:controller="com.nexus.desktop.PrimaryController">
    <children>
        <Label fx:id="welcomeText" />
        <Button fx:id="loginButton" text="Login" onAction="#onLoginButtonClick" />
        <Button fx:id="exitButton" text="Exit" onAction="#onExitButtonClick" />
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>